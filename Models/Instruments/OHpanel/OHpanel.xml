<!-- Boeing 717-200 for FlightGear -->
<!-- Overhead Panel -->

<PropertyList>

	<path>OHpanel.ac</path>

	<!-- ANIMATIONS -->
	<!-- APU fuel pump switch -->
	<animation>
		<name>APU fuel pump switch pick</name>
		<type>pick</type>
		<object-name>APUAirSwtch</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/APU/fuel-pump</property>
			</binding>
		</action>
	</animation>
	<animation>
		<name>APU fuel pump switch rotate</name>
		<type>rotate</type>
		<object-name>APUAirSwtch</object-name>
		<property>controls/APU/fuel-pump</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>30</dep></entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.029</z-m>
		</center>
	</animation>

